    }
void ConstruirCampo()
    {
    for (int i =0; i<40; i++)
        {
        for (int j =0; j<20; j++)
            {
            Campo[i][j] = '.';

            }
        }
    }

void PrintarCampo()
    {
    system("cls");
    char bloco = 219;
    for (int i =0; i<42; i++)
        {
        printf("                                                                ");
        for (int j =0; j<22; j++)
            {
            if(i==0 && j==0){printf("%c", 218);}//canto esquerdo superior
            else if(i==0 && j==21)//canto direito superior
                {
                printf("%c", 191);
                printf("     %d pontos", pontos);
                }
            else if(i==41 && j==0)//canto esquerdo inferior
                {
                printf("%c", 192);
                }
            else if(i==41 && j==21)//canto direito inferior
                {
                printf("%c", 217);
                }

            else if(i==0 || i==41)//bordas horizontais
                {
                printf("%c", 196);
                printf("%c", 196);
                }
            else if(j==0 || j==21)//bordas verticais
                {
                printf("%c", 179);
                }



            else
                {
                    if(Campo[i-1][j-1]=='.')//espacos vazios
                        {
                        printf(" ");
                        printf(" ");
                        }
                    else//pecas
                        {
                        printf("%c", bloco);
                        printf("%c", bloco);
                        }
                }

            }
        printf("\n");

        }
    }

void MesclarPecaCampo()
    {
    for(int i=0; i<40; i++)
        {
        for(int j=0; j<20; j++)
            {
            if(Campo[i][j]=='X')
                {
                Campo[i][j] = '.';
                }
//            else{}
            }
        }

    for (int i=0; i<8; i++)//posiciona a nova peca
        {
        for (int j=0; j<8; j++)
            {
            if(PecaAtual.PecaArray[i][j]=='X')
                {
                 Campo[i+PecaAtual.PosicaoY][j+PecaAtual.PosicaoX]='X';
                }
            }
        }
    }
void SalvarCampo()
    {
        for(int i=0; i<40; i++)
        {
        for(int j=0; j<20; j++)
            {
            if(Campo[i][j]=='X')
                {
                Campo[i][j] = 'Y';
                }
  //          else{}
            }
        }
    }
int Mover(char direcao)
    {



    char PecaTemporaria[8][8];
    if(direcao=='w')
        {
        if(ChecarSeCabe('w'))
            {
                PecaTemporaria[0][0]=PecaAtual.PecaArray[7][0];
                PecaTemporaria[0][1]=PecaAtual.PecaArray[6][0];
                PecaTemporaria[0][2]=PecaAtual.PecaArray[5][0];
                PecaTemporaria[0][3]=PecaAtual.PecaArray[4][0];
                PecaTemporaria[0][4]=PecaAtual.PecaArray[3][0];
                PecaTemporaria[0][5]=PecaAtual.PecaArray[2][0];
                PecaTemporaria[0][6]=PecaAtual.PecaArray[1][0];
                PecaTemporaria[0][7]=PecaAtual.PecaArray[0][0];
                PecaTemporaria[1][0]=PecaAtual.PecaArray[7][1];
                PecaTemporaria[1][1]=PecaAtual.PecaArray[6][1];
                PecaTemporaria[1][2]=PecaAtual.PecaArray[5][1];
                PecaTemporaria[1][3]=PecaAtual.PecaArray[4][1];
                PecaTemporaria[1][4]=PecaAtual.PecaArray[3][1];
                PecaTemporaria[1][5]=PecaAtual.PecaArray[2][1];
                PecaTemporaria[1][6]=PecaAtual.PecaArray[1][1];
                PecaTemporaria[1][7]=PecaAtual.PecaArray[0][1];
                PecaTemporaria[2][0]=PecaAtual.PecaArray[7][2];
                PecaTemporaria[2][1]=PecaAtual.PecaArray[6][2];
                PecaTemporaria[2][2]=PecaAtual.PecaArray[5][2];
                PecaTemporaria[2][3]=PecaAtual.PecaArray[4][2];
                PecaTemporaria[2][4]=PecaAtual.PecaArray[3][2];
                PecaTemporaria[2][5]=PecaAtual.PecaArray[2][2];
                PecaTemporaria[2][6]=PecaAtual.PecaArray[1][2];
                PecaTemporaria[2][7]=PecaAtual.PecaArray[0][2];
                PecaTemporaria[3][0]=PecaAtual.PecaArray[7][3];
                PecaTemporaria[3][1]=PecaAtual.PecaArray[6][3];
                PecaTemporaria[3][2]=PecaAtual.PecaArray[5][3];
                PecaTemporaria[3][3]=PecaAtual.PecaArray[4][3];
                PecaTemporaria[3][4]=PecaAtual.PecaArray[3][3];
                PecaTemporaria[3][5]=PecaAtual.PecaArray[2][3];
                PecaTemporaria[3][6]=PecaAtual.PecaArray[1][3];
                PecaTemporaria[3][7]=PecaAtual.PecaArray[0][3];
                PecaTemporaria[4][0]=PecaAtual.PecaArray[7][4];
                PecaTemporaria[4][1]=PecaAtual.PecaArray[6][4];
                PecaTemporaria[4][2]=PecaAtual.PecaArray[5][4];
                PecaTemporaria[4][3]=PecaAtual.PecaArray[4][4];
                PecaTemporaria[4][4]=PecaAtual.PecaArray[3][4];
                PecaTemporaria[4][5]=PecaAtual.PecaArray[2][4];
                PecaTemporaria[4][6]=PecaAtual.PecaArray[1][4];
                PecaTemporaria[4][7]=PecaAtual.PecaArray[0][4];
                PecaTemporaria[5][0]=PecaAtual.PecaArray[7][5];
                PecaTemporaria[5][1]=PecaAtual.PecaArray[6][5];
                PecaTemporaria[5][2]=PecaAtual.PecaArray[5][5];
                PecaTemporaria[5][3]=PecaAtual.PecaArray[4][5];
                PecaTemporaria[5][4]=PecaAtual.PecaArray[3][5];
                PecaTemporaria[5][5]=PecaAtual.PecaArray[2][5];
                PecaTemporaria[5][6]=PecaAtual.PecaArray[1][5];
                PecaTemporaria[5][7]=PecaAtual.PecaArray[0][5];
                PecaTemporaria[6][0]=PecaAtual.PecaArray[7][6];
                PecaTemporaria[6][1]=PecaAtual.PecaArray[6][6];
                PecaTemporaria[6][2]=PecaAtual.PecaArray[5][6];
                PecaTemporaria[6][3]=PecaAtual.PecaArray[4][6];
                PecaTemporaria[6][4]=PecaAtual.PecaArray[3][6];
                PecaTemporaria[6][5]=PecaAtual.PecaArray[2][6];
                PecaTemporaria[6][6]=PecaAtual.PecaArray[1][6];
                PecaTemporaria[6][7]=PecaAtual.PecaArray[0][6];
                PecaTemporaria[7][0]=PecaAtual.PecaArray[7][7];
                PecaTemporaria[7][1]=PecaAtual.PecaArray[6][7];
                PecaTemporaria[7][2]=PecaAtual.PecaArray[5][7];
                PecaTemporaria[7][3]=PecaAtual.PecaArray[4][7];
                PecaTemporaria[7][4]=PecaAtual.PecaArray[3][7];
                PecaTemporaria[7][5]=PecaAtual.PecaArray[2][7];
                PecaTemporaria[7][6]=PecaAtual.PecaArray[1][7];
                PecaTemporaria[7][7]=PecaAtual.PecaArray[0][7];





                for(int i=0; i<8; i++)
                {
                    for(int j=0; j<8; j++)
                        {
                        PecaAtual.PecaArray[i][j] = PecaTemporaria[i][j];
                        }
                }
            }
        return 0;
        }

    if(direcao=='s')//descer peca
        {
        if(ChecarSeCabe('s'))
            {
            PecaAtual.PosicaoY = PecaAtual.PosicaoY + 2;
            return 0;
            }
        else
            {
            SalvarCampo();
            return 1;
            }
        }

    if(direcao=='a')//mover peca para esquerda
        {
        if(ChecarSeCabe('a'))
            {
            PecaAtual.PosicaoX = PecaAtual.PosicaoX - 2;
            }
        return 0;
        }

    if(direcao=='d')//mover peca para direita
        {
        if(ChecarSeCabe('d'))
            {
             PecaAtual.PosicaoX = PecaAtual.PosicaoX + 2;
            }
        return 0;
        }
    if(direcao==' ')//desces a peca instantaniamente
        {
        int PecaCaiu=0;
        while(!PecaCaiu)
            {
            PecaCaiu = Mover('s');
            MesclarPecaCampo();
            }
        }

    return 0;
    }
void EliminarLinhasCompletas()
    {
    int contador= 0;
    for (int i =0; i<40; i++)
        {
        int contador= 0;
        for (int j =0; j<20; j++)
            {
            if(Campo[i][j]!='.'){contador = contador+1;}
            }
        if(contador==20)//entra nesse if caso a linha esteja completa
            {
            EliminarLinha(i);//elimina a linha que esta completa
            RemanejarCampo(i);//remaneja as peca que estao acima da linha eliminada
            }
        }
    }
void EliminarLinha(int linha)
    {
    for(int j=0; j<20; j++){Campo[linha][j]='.';}
    pontos = pontos +100;
    }
void RemanejarCampo(int linha)
    {
    for (int i =linha-1; i>=0; i--)
        {
        for (int j =0; j<20; j++)
            {
            if(Campo[i][j]!= '.')
                {
                Campo[i+1][j]=Campo[i][j];
                Campo[i][j]='.';
                }
            }
        }
    }
void GameOver()
    {
    system ("cls");
    printf("\n\n\n\n");
    printf("                                             _____                                       _____\n");
    printf("                                            /  ___|                                     /     \\ \n");
    printf("                                            | /        ______    _     _    _____      |  __  | __     _   _____    _____\n");
    printf("                                            | |  __   / _____\\  / \\   / \\  / ___ \\     | |  | | | |   | | / ___ \\  | ___ \\ \n");
    printf("                                            | | |_ \\  | |___| | |   \\/ / | |  ___/     | |__| | | |   | | |  ___/  |  __ /\n");
    printf("                                            \\ \\__| |  | |   | | | |\\__/| | | |____     |      |  \\ \\_/ /  | |____  | |  \\ \\ \n");
    printf("                                             \\_____/  |_|   |_| |_|    |_|  \\____/      \\____/    \\___/    \\____/  |_|   \\_|\n\n\n");
    printf("                                                                    Voce fez %d pontos\n", pontos);
    printf("                                                                    Aperte enter para jogar novamente.\n");
    char a;
    scanf("%c",&a);
    system ("cls");
    main();
    }

int ChecarSeCabe(char direcao)
    {

    Peca PecaTemporaria = PecaAtual;
    if (direcao=='w')//girar horario
        {
                PecaTemporaria.PecaArray[0][0]=PecaAtual.PecaArray[7][0];
                PecaTemporaria.PecaArray[0][1]=PecaAtual.PecaArray[6][0];
                PecaTemporaria.PecaArray[0][2]=PecaAtual.PecaArray[5][0];
                PecaTemporaria.PecaArray[0][3]=PecaAtual.PecaArray[4][0];
                PecaTemporaria.PecaArray[0][4]=PecaAtual.PecaArray[3][0];
                PecaTemporaria.PecaArray[0][5]=PecaAtual.PecaArray[2][0];
                PecaTemporaria.PecaArray[0][6]=PecaAtual.PecaArray[1][0];
                PecaTemporaria.PecaArray[0][7]=PecaAtual.PecaArray[0][0];
                PecaTemporaria.PecaArray[1][0]=PecaAtual.PecaArray[7][1];
                PecaTemporaria.PecaArray[1][1]=PecaAtual.PecaArray[6][1];
                PecaTemporaria.PecaArray[1][2]=PecaAtual.PecaArray[5][1];
                PecaTemporaria.PecaArray[1][3]=PecaAtual.PecaArray[4][1];
                PecaTemporaria.PecaArray[1][4]=PecaAtual.PecaArray[3][1];
                PecaTemporaria.PecaArray[1][5]=PecaAtual.PecaArray[2][1];
                PecaTemporaria.PecaArray[1][6]=PecaAtual.PecaArray[1][1];
                PecaTemporaria.PecaArray[1][7]=PecaAtual.PecaArray[0][1];
                PecaTemporaria.PecaArray[2][0]=PecaAtual.PecaArray[7][2];
                PecaTemporaria.PecaArray[2][1]=PecaAtual.PecaArray[6][2];
                PecaTemporaria.PecaArray[2][2]=PecaAtual.PecaArray[5][2];
                PecaTemporaria.PecaArray[2][3]=PecaAtual.PecaArray[4][2];
                PecaTemporaria.PecaArray[2][4]=PecaAtual.PecaArray[3][2];
                PecaTemporaria.PecaArray[2][5]=PecaAtual.PecaArray[2][2];
                PecaTemporaria.PecaArray[2][6]=PecaAtual.PecaArray[1][2];
                PecaTemporaria.PecaArray[2][7]=PecaAtual.PecaArray[0][2];
                PecaTemporaria.PecaArray[3][0]=PecaAtual.PecaArray[7][3];
                PecaTemporaria.PecaArray[3][1]=PecaAtual.PecaArray[6][3];
                PecaTemporaria.PecaArray[3][2]=PecaAtual.PecaArray[5][3];
                PecaTemporaria.PecaArray[3][3]=PecaAtual.PecaArray[4][3];
                PecaTemporaria.PecaArray[3][4]=PecaAtual.PecaArray[3][3];
                PecaTemporaria.PecaArray[3][5]=PecaAtual.PecaArray[2][3];
                PecaTemporaria.PecaArray[3][6]=PecaAtual.PecaArray[1][3];
                PecaTemporaria.PecaArray[3][7]=PecaAtual.PecaArray[0][3];
                PecaTemporaria.PecaArray[4][0]=PecaAtual.PecaArray[7][4];
                PecaTemporaria.PecaArray[4][1]=PecaAtual.PecaArray[6][4];
                PecaTemporaria.PecaArray[4][2]=PecaAtual.PecaArray[5][4];
                PecaTemporaria.PecaArray[4][3]=PecaAtual.PecaArray[4][4];
                PecaTemporaria.PecaArray[4][4]=PecaAtual.PecaArray[3][4];
                PecaTemporaria.PecaArray[4][5]=PecaAtual.PecaArray[2][4];
                PecaTemporaria.PecaArray[4][6]=PecaAtual.PecaArray[1][4];
                PecaTemporaria.PecaArray[4][7]=PecaAtual.PecaArray[0][4];
                PecaTemporaria.PecaArray[5][0]=PecaAtual.PecaArray[7][5];
                PecaTemporaria.PecaArray[5][1]=PecaAtual.PecaArray[6][5];
                PecaTemporaria.PecaArray[5][2]=PecaAtual.PecaArray[5][5];
                PecaTemporaria.PecaArray[5][3]=PecaAtual.PecaArray[4][5];
                PecaTemporaria.PecaArray[5][4]=PecaAtual.PecaArray[3][5];
                PecaTemporaria.PecaArray[5][5]=PecaAtual.PecaArray[2][5];
                PecaTemporaria.PecaArray[5][6]=PecaAtual.PecaArray[1][5];
                PecaTemporaria.PecaArray[5][7]=PecaAtual.PecaArray[0][5];
                PecaTemporaria.PecaArray[6][0]=PecaAtual.PecaArray[7][6];
                PecaTemporaria.PecaArray[6][1]=PecaAtual.PecaArray[6][6];
                PecaTemporaria.PecaArray[6][2]=PecaAtual.PecaArray[5][6];
                PecaTemporaria.PecaArray[6][3]=PecaAtual.PecaArray[4][6];
                PecaTemporaria.PecaArray[6][4]=PecaAtual.PecaArray[3][6];
                PecaTemporaria.PecaArray[6][5]=PecaAtual.PecaArray[2][6];
                PecaTemporaria.PecaArray[6][6]=PecaAtual.PecaArray[1][6];
                PecaTemporaria.PecaArray[6][7]=PecaAtual.PecaArray[0][6];
                PecaTemporaria.PecaArray[7][0]=PecaAtual.PecaArray[7][7];
                PecaTemporaria.PecaArray[7][1]=PecaAtual.PecaArray[6][7];
                PecaTemporaria.PecaArray[7][2]=PecaAtual.PecaArray[5][7];
                PecaTemporaria.PecaArray[7][3]=PecaAtual.PecaArray[4][7];
                PecaTemporaria.PecaArray[7][4]=PecaAtual.PecaArray[3][7];
                PecaTemporaria.PecaArray[7][5]=PecaAtual.PecaArray[2][7];
                PecaTemporaria.PecaArray[7][6]=PecaAtual.PecaArray[1][7];
                PecaTemporaria.PecaArray[7][7]=PecaAtual.PecaArray[0][7];

        for(int i=0; i<8; i++)
            {
            for(int j=0; j<8; j++)
                {
                if((PecaTemporaria.PecaArray[i][j]=='X') && (Campo[i+PecaTemporaria.PosicaoY][j+PecaTemporaria.PosicaoX]=='Y')){return 0;}//verifica se a peca ocuparia posicao ja ocupada por pecas antigas
                if((PecaTemporaria.PecaArray[i][j]=='X') && ((i+PecaTemporaria.PosicaoY)>=40 || (i+PecaTemporaria.PosicaoY)<0)){return 0;}//verifica se a peca ultrapaca os limites em Y
                if((PecaTemporaria.PecaArray[i][j]=='X') && ((j+PecaTemporaria.PosicaoX)>=20 || (j+PecaTemporaria.PosicaoX)<0)){return 0;}//verifica se a peca ultrapaca os limites em X
                }
            }
    return 1;
        }
    if (direcao=='s')//descer uma casa
        {
        PecaTemporaria.PosicaoY = PecaTemporaria.PosicaoY + 2;
        for(int i=0; i<8; i++)
            {
            for(int j=0; j<8; j++)
                {
                if ((PecaTemporaria.PecaArray[i][j]=='X') && (Campo[i+PecaTemporaria.PosicaoY][j+PecaTemporaria.PosicaoX]=='Y')){return 0;}//verifica se a peca ocuparia posicao ja ocupada por pecas antigas
                if ((PecaTemporaria.PecaArray[i][j]=='X') && ((i+PecaTemporaria.PosicaoY)>=40 || (i+PecaTemporaria.PosicaoY)<0)){return 0;}//verifica se a peca ultrapaca os limites em Y
                }
            }
        return 1;
        }

    if (direcao=='a')//mover esquerda
        {
        PecaTemporaria.PosicaoX = PecaTemporaria.PosicaoX - 2;
        for(int i=0; i<8; i++)
            {
                for(int j=0; j<8; j++)
                    {
                    if((PecaTemporaria.PecaArray[i][j]=='X') && (Campo[i+PecaTemporaria.PosicaoY][j+PecaTemporaria.PosicaoX]=='Y')){return 0;}//verifica se a peca ocuparia posicao ja ocupada por pecas antigas
                    if((PecaTemporaria.PecaArray[i][j]=='X') &&  (j+PecaTemporaria.PosicaoX) < 0){return 0;}//verifica se a peca ultrapaca os limites em X
                    }

            }
        return 1;
        }

    if (direcao=='d')//mover direita
        {
        PecaTemporaria.PosicaoX = PecaTemporaria.PosicaoX + 2;
        for(int i=0; i<8; i++)
            {
                for(int j=0; j<8; j++)
                    {
                    if((PecaTemporaria.PecaArray[i][j]=='X') && (Campo[i+PecaTemporaria.PosicaoY][j+PecaTemporaria.PosicaoX]=='Y')){return 0;}//verifica se a peca ocuparia posicao ja ocupada por pecas antigas
                    if((PecaTemporaria.PecaArray[i][j]=='X') && ((j+PecaTemporaria.PosicaoX)>=20 || (j+PecaTemporaria.PosicaoX)<0)){return 0;}//verifica se a peca ultrapaca os limites em X
                    }

            }
        return 1;
        }
    }
